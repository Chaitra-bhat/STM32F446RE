#ifndef STM32F446RE_STDTYPES_H_
#define STM32F446RE_STDTYPES_H_

#define ENABLE		1U
#define DISABLE		0U
#define SET			ENABLE
#define RESET		DISABLE

/* macros for enabling or disabling a function*/
#define PROJECT_LED			0U
#define PROJECT_BUTTON		1U

#endif /* STM32F446RE_STDTYPES_H_ */
